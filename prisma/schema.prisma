generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model accounts {
  id          BigInt  @id(map: "account_id") @default(autoincrement())
  user_id     BigInt
  provider    String?
  provider_id String?
  users       users   @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "user_id")
}

model users {
  id            BigInt     @id(map: "user_id") @default(autoincrement())
  email         String    @unique(map: "email")
  password_hash String?
  name          String
  accounts      accounts[]
}
